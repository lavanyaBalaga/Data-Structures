#include<stdio.h>
int main()
{
    int a[100],b[100],i,p=0,q=0;
    
    for(i=0;i<3;i++)
       scanf("%d",&a[i]);
    for(i=0;i<3;i++)
       scanf("%d",&b[i]);
    for(i=0;i<3;i++)
    {
        if(a[i]>b[i])
          p=p+1;
        else if(b[i]>a[i])
          q=q+1;
    }
    printf("%d\t%d",p,q);
}
       
